{
  "presets": ["react"],
  "env": {
    "web": {
      "presets": ["react-hmre"],
      "plugins": [
        "transform-es2015-classes",
        "transform-object-rest-spread"
      ]
    },
    "dev": {
      "presets": [
        ["env", {
          "targets": {
            "browsers": ["last 3 versions"]
          },
          "modules": false
        }]
      ],
      "plugins": [
        "transform-es2015-classes",
        "transform-object-rest-spread"
      ]
    },
    "production": {
      "presets": [
        ["env", {
          "targets": {
            "browsers": ["last 3 versions"]
          },
          "modules": false
        }]
      ],
      "plugins": [
        "transform-object-rest-spread"
      ]
    }
  }
}